{
  "/UserScore/scoreTransMoney": {
    "post": {
      "summary": "用户积分转为用户余额",
      "description": "用户积分转为用户余额 10:1",
      "consumes": [
        "application/x-www-form-urlencoded"
      ],
      "produces": [
        "application/json"
      ],
      "parameters": [
        {
          "$ref": "#/declares/base.session_ticket"
        },
        {
          "in": "formData",
          "type": "integer",
          "required": true,
          "name": "score",
          "description": "要转化的积分数量"
        }
      ],
      "responses": {
        "200": {
          "type": "object",
          "properties": {
            "code": {
              "$ref": "#/declares/base.code"
            },
            "msg": {
              "$ref": "#/declares/base.msg"
            },
            "data": {
              "type": "object",
              "properties": {
                "list": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "title": {
                        "type": "string",
                        "description": "标题"
                      },
                      "score_type": {
                        "type": "integer",
                        "description": "积分类型, 1:收入, 2:支出"
                      },
                      "score": {
                        "type": "integer",
                        "description": "积分(正数)"
                      },
                      "expire_time": {
                        "type": "integer",
                        "description": "过期时间"
                      },
                      "create_time": {
                        "type": "integer",
                        "description": "创建时间"
                      }
                    }
                  }
                },
                "page": {
                  "$ref": "#/declares/common.page"
                }
              }
            }
          }
        }
      }
    }
  }
}